(include_subdirs unqualified)

(library
 (name common)
 (ocamlopt_flags -O3 -inline-max-depth=5 -inline-max-unroll=2)
 (flags -w -20-33-3)
 (preprocess
  (pps ppx_log))
 (libraries ocamlgraph ctypes ctypes-foreign ppx_log stdlibExt)
 (modules
  ;; syntax-core
  Const
