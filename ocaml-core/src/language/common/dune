  InstFullF
  RawInst
  ;; syntax-jmp
  CallTargetF
  JIntra
  JCallF
  JRetF
  JTailCallF
  JmpFullF
  ;; syntax-block
  BlockF
  ;; syntax-func
  FuncHelperF
  ;; semantics-timestamp
  Int64TimeStamp
  UnitTimeStamp
