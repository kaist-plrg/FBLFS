(rule
 (with-stdout-to
  DisassemblyTest1.output
  (run
   ../../src/tool/slaparser.exe
   -dasm
   "55"
   "4889e5"
   "4883ec10"
   "c745fc00000000"
   "8345fc01"
   "8b45fc"
   "89c6"
   "b800000000"
   "90"
   "c9"
   "c3")))

(rule
 (alias runtest)
 (action
  (diff DisassemblyTest1.expected DisassemblyTest1.output)))

